{
  "welcomeFile": "/exer07-ui/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/northwind/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "northwind",
      "csrfProtection": false
    },
    {
      "source": "^/exer07-ui/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}